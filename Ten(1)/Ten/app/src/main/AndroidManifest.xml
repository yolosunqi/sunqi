<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ten"
    android:installLocation="auto">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="18990091-毕竞之"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name=".BannerPagerActivity"
            android:exported="true"
            android:theme="@style/AppCompatTheme" />
        <activity
            android:name=".TabFragmentActivity"
            android:exported="true"
            android:theme="@style/AppCompatTheme" />
        <activity
            android:name=".TabLayoutActivity"
            android:exported="true"
            android:theme="@style/AppCompatTheme" />

        <activity android:name=".ViewFlipperActivity"
            android:exported="true"></activity>

        <activity
            android:name=".ToolbarActivity"
            android:exported="true"
            android:theme="@style/AppCompatTheme" />

        <activity
            android:name=".OverflowMenuActivity"
            android:exported="true"
            android:theme="@style/AppCompatTheme" />

        <activity
            android:name=".SearchResultActvity"
            android:theme="@style/AppCompatTheme">
            <intent-filter>
                <action android:name="android.intent.action.SEARCH" />
            </intent-filter>

            <meta-data
                android:name="android.app.searchable"
                android:resource="@xml/searchable" />
        </activity>
        <activity
            android:name=".SearchViewActivity"
            android:exported="true"
            android:theme="@style/AppCompatTheme" />


        <activity android:name=".TabFirstActivity"/>
        <activity android:name=".TabSecondActivity" />
        <activity android:name=".TabThirdActivity"/>

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>